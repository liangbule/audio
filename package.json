{
  "name": "audio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.js --platform ios --dev false --bundle-output ./ios/bundle/index.ios.bundle --assets-dest ./ios/bundle",
    "bundle-android": "react-native bundle --entry-file index.js --platform android --dev false --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/assets/res/",
    "web_dev": "cd build && webpack serve --config webpack.config.js",
    "web_build": "cd build && webpack --mode=production --config webpack.config.js",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "cache": "npm start -- --reset-cache"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-art": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@babel/core": "7.20.0",
    "@babel/plugin-transform-runtime": "7.23.2",
    "@babel/preset-env": "7.20.0",
    "@babel/preset-typescript": "7.23.2",
    "@babel/runtime": "7.20.0",
    "@react-native/eslint-config": "0.72.2",
    "@react-native/metro-config": "0.72.11",
    "@tsconfig/react-native": "3.0.0",
    "@types/react": "18.0.24",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "29.2.1",
    "babel-loader": "9.1.3",
    "eslint": "8.19.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.3",
    "jest": "29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "mini-css-extract-plugin": "2.7.6",
    "prettier": "2.4.1",
    "react-native-web": "0.19.9",
    "react-test-renderer": "18.2.0",
    "ts-loader": "9.5.0",
    "typescript": "4.8.4",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "engines": {
    "node": ">=16"
  }
}
